{
    "name": "topthink/think-helper",
    "description": "The ThinkPHP6 Helper Package For PHP5.6",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "axguowen",
            "email": "axguowen@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors"
    },
    "scripts-descriptions": {
        "test": "Run all tests."
    }
}
